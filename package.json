{
  "name": "react-native-a11y-order",
  "version": "0.8.0-rc2",
  "description": "ReactNative library for managing screen reader focus ordering",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace a11y-order-example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "release": "release-it --only-version",
    "release-ni": "release-it --no-increment"
  },
  "keywords": [
    "react-native",
    "accessibility",
    "screen reader",
    "focus order",
    "VoiceOver",
    "TalkBack",
    "ios",
    "android",
    "WCAG",
    "a11y"
  ],
  "repository": "https://github.com/ArturKalach/react-native-a11y-order",
  "author": "Artur Kalach <artur.kalach.andreevich@gmail.com> (https://github.com/ArturKalach)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArturKalach/react-native-a11y-order/issues"
  },
  "homepage": "https://github.com/ArturKalach/react-native-a11y-order#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "lefthook": "^2.0.3",
    "prettier": "^2.8.8",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.40.18",
    "release-it": "^19.0.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNA11yOrderSpec",
    "type": "all",
    "jsSrcsDir": "src",
    "ios": {
      "componentProvider": {
        "A11yGroupView": "RNAOA11yGroupView",
        "A11yIndexView": "RNAOA11yIndexView",
        "A11yOrderView": "RNAOA11yOrderView",
        "A11yUIContainer": "RNAOA11yUIContainerView",
        "A11yPaneTitle": "RNAOA11yPaneTitleView",
        "A11yView": "RNAOA11yView",
        "A11yLock": "RNAOA11yLockView"
      },
      "modulesProvider": {
        "A11yAnnounceModule": "RNAOA11yAnnounceModule"
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  }
}
